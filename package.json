    "@vitejs/plugin-react": "1.1.3",
    "eslint-config-prettier": "8.3.0",
    "prettier": "2.5.1",
    "typescript": "4.5.4",
    "vite": "2.7.10",
    "vite-plugin-checker": "0.3.4"
  }
